{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "experimentalDecorators": true,
      "moduleResolution": "node",
      "rootDir": "src",
      "outDir": "dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
    },
    "include": ["src"]
  }
  